/*just for start level*/

.star-image {
   width: 30rpx;
   height: 30rpx;
}

.image {
   margin:15%;
   width: 70%;
   height: 70%;
}

/*tab导航*/
.swiper-item {
   display: block;
   height: 300rpx;
}

.addDishAnimation {
   position: absolute;
   width: 20rpx;
   height: 20rpx;
   border-radius: 50%;
   background-color: #c66;
}

.searchImg {
   width: 40rpx;
   height: 35rpx;
   margin: 15rpx 20rpx 0 0;
   background-color: #fff;
}

.search-food-input {
   background-color: #fff;
   text-align: center;
   width: 100%;
   height: 100%;
   font-size: 28rpx;
   color: #c55;
}

.searchedFoods {
   border-bottom: 5rpx solid #c44;
   width: 94%;
   height: 960rpx;
   position: absolute;
   padding:0 3%;
   top: 100rpx;
   z-index: 1000;
   background-color: rgba(255, 255, 255, 0.9);
}

.searchedFoods-li {
   height: 80rpx;
   line-height: 80rpx;
   border-bottom: 2rpx solid #c54;
}

.searched-img {
   display: inline-block;
   width: 120rpx;
   height: 120rpx;
}

.searched-name {
   display: inline-block;
   width: 240rpx;
}

.searched-price {
   display: inline-block;
   width: 140rpx;
}

.searched-sales {
   display: inline-block;
   width: 140rpx;
}

.searched-tab {
   display: inline-block;
}

.searched-count-wrap {
   float: right;
}

.searched-count {
   float: right;
   padding: 0 10rpx 0 5rpx;
}

.searched-tab-img {
   width: 50rpx;
   height: 50rpx;
}



.black-wall{
   position:fixed;
   width:100%;
   height:100%;
   z-index: 10;
   background:rgba(50, 50, 50, 0.85);
}

.chosenFoods-part {
   position: fixed;
   bottom: 10%;
   width:90%;
   margin:5%;
   border-radius: 5px;
   box-shadow: 0 0 10rpx #999;
   background:rgba(50, 50, 50, 0.7);
   z-index: 10000;
}

.chosenFoods-title {
   font-size: 30rpx;
   text-align: left;
   font-weight: 600;
   padding-left:10px;
   height:40px;
   line-height: 40px;
   background-color:#FFF;
   border-top-left-radius: 5px;
   border-top-right-radius: 5px;
   border-bottom: 1px solid #999;
}

.chosenFoods{
   max-height: 500rpx;
   overflow-y: scroll;
   background:#FFF;
   border-bottom-left-radius: 5px;
   border-bottom-right-radius: 5px;
}
 .theCoupon{
   color:#d14450;
   margin-left:10px;
   font-weight: 300;
   margin-right:10px;
   height:30px;
   line-height: 30px;
}

 .theCoupon span{
   float:right;
   font-weight:600;
}

.chosenFoods-li {
   padding:0 2%;
   height: 70rpx;
   width: 96%;
}

.chosenFoods-li-name {
   display: inline-block;
   overflow: hidden;
   margin-left: 5rpx;
   transform: translateY(5rpx);
}

.chosenFoods-li-price {
   float:right;
   margin-right:40px;
   padding-top:2px;
}

.chosenFoods-li-count-wrp {
   float:right;
}

.chosenFoods-li-count {
   display: inline-block;
   width: 30rpx;
   margin: 0 10rpx;
   transform: translateY(-10rpx);
   text-align: center;
}

.totalImg {
   width: 50rpx;
   height: 50rpx;
}

.nav-tab-wrp {
   height: 70rpx;
   width: 96%;
   margin:2%;
   display: flex;
   flex-direction: row;
   background: rgba(250, 250, 250, 1);
   position: fixed;
   top: -1rpx;
   border: 2rpx solid #D55;
   z-index: 10;
   border-radius: 5px;
   overflow: hidden;
}

.nav-tab-item {
   width: 250rpx;
   text-align: center;
   line-height: 88rpx;
   color: rgb(102, 102, 102);
}

/*底部浮动*/

.cart-wrp {
   height: 90rpx;
   width:90%;
   transform: translateX(-50%);
   left:50%;
   border-radius:5px;
   position: fixed;
   bottom: 2%;
   background: rgba(50, 50, 50, 0.8);
   z-index: 10;
   color: black;
}

.cart-total {
   display: inline-block;
   padding-left: 30rpx;
}

.cart-icon-wrp {
   position: absolute;
   top: -4rpx;
   transform:translateY(-10px);
}

.cart-icon {
   margin-top: 5rpx;
   width: 90rpx;
   height: 90rpx;
   transition: 0.5s;
}

.cart-total-num {
   display: inline-block;
   width: 36rpx;
   height: 36rpx;
   font-size: 22rpx;
   color: white;
   line-height: 36rpx;
   border-radius: 50%;
   text-align: center;
   position: absolute;
   right: 2rpx;
}

.cart-total-price {
   display: inline-block;
   width: 40rpx;
   height: 40rpx;
   font-size: 36rpx;
   color: #ed7e05;
   line-height: 100rpx;
   text-align: center;
   padding-left: 110rpx;
}

.cart-item-btn {
   width:80px;
   height:100%;
   line-height: 100rpx;
   float: right;
   border-top-right-radius: 5px;
   border-bottom-right-radius: 5px;
   font-size: 28rpx;
   background: #f5a22d;
   color: #fff;
   text-align: center;
}

/*菜单wrap*/

.menu-wrp {
   /* position:absolute; *//* transform:translateY(-1000rpx);  */
   overflow: hidden;
   width: 750rpx;
   height:980rpx;
   display: flex;
   padding: 100rpx 0; 
   background-color: white;
}

/*左侧菜单*/

.menu-item-wrp {
   text-align: center;
}

.menu-item {
   width: 160rpx;
   height: 200rpx;
   color: #fff;
   display: flex;
   justify-content: center;
   align-items: center;
}

.m-item-icon {
   width: 100rpx;
   height: 100rpx;
   margin: 0 auto;
   border-radius: 50%;
   background:linear-gradient()
}

.m-item-title {
   font-size: 24rpx;
   color: black;
}

.left-side {
   height: 100%;
   padding-bottom: 88rpx;
   display: flex;
   width: 160rpx;
   background-color: #fff;
   flex-direction: column;
}

/*右侧菜品*/

.dish-category-title {
   font-size: 32rpx;
   color: #888;
   padding: 20rpx 0;
   margin-bottom: 6rpx;
   border-bottom:1px solid red;
}

.right-side {
   width: 100%;
   height: 100%;
   color: #fff;
   justify-content: center;
   align-items: center;
}

.menu-footer {
   height: 100rpx;
}

/*菜品信息*/

.dish-item {
   width: 100%;
   padding: 6rpx 0;
}

.dish-item-wrp {
   width: 100%;
   display: flex;
   flex-direction: row;
}

.dish-item-pic {
   padding:0;
   width: 158rpx;
   margin-right:20rpx;
}

.dish-item-pic image {
   width: 158rpx;
   height: 140rpx;
}

.dish-item-info {
   width: 150rpx;
   margin-top:20rpx;
   padding-left: 16rpx;
   padding-top: 0;
}

.dish-item-name {
   color: #444;
   font-size: 26rpx;
}

.dish-item-star {
   height: 40rpx;
   line-height: 40rpx;
}

.dish-item-sales {
   color: #ccc;
   font-size: 20rpx;
}

.dish-item-money {
   color: #f50909;
   font-size: 26rpx;
}

.dish-footer {
   background: #c22;
   font-size: 26rpx;
   text-align: center;
   height: 10rpx;
   line-height: 50rpx;
}

/*操作菜品数量*/
.dish-item-count {
   float:right;
   width: 220rpx;
   font-size: 26rpx;
   padding: 50rpx 0 0 20rpx;
}

.dish-item-tab {
   width: 60rpx;
   height: 60rpx;
}

.dish-item-count-minus,.dish-item-count-plus  {
   float: right;
   width: 20px;
   height: 20px;
   margin:15rpx;
   line-height: 20px;
   text-align: center;
   border-radius: 5px;
   background-color:#f94e5b;
   font-size:36rpx;
   font-weight: 600;
   color:#FFF;
}

.dish-item-count-text {
   float: right;
   color: #333;
   margin:13rpx 10rpx 0 10rpx;
   font-size: 28rpx;
   line-height: 45rpx;
   text-align: center;
}

.index {
   background-color: #fbf9fe;
   font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
   max-height: 100%;
   font-size: 32rpx;
   overflow: hidden;
}

page {
   background-color: white;
   height: 100%;
   overflow: hidden;
}

.container {
   display: flex;
   flex-direction: column;
   min-height: 100%;
   justify-content: space-between;
}

.page-header {
   display: flex;
   font-size: 32rpx;
   color: #aaa;
   margin-top: 50rpx;
   flex-direction: column;
   align-items: center;
}

.page-header-text {
   padding: 20rpx 40rpx;
}

.page-header-line {
   width: 150rpx;
   height: 1px;
   border-bottom: 1px solid #ccc;
}

.page-body {
   width: 100%;
   display: flex;
   flex-direction: column;
   align-items: center;
   flex-grow: 1;
   overflow-x: hidden;
}

.page-body-wrapper {
   margin-top: 100rpx;
   display: flex;
   flex-direction: column;
   align-items: center;
   width: 100%;
}

.page-body-wrapper form {
   width: 100%;
}

.page-body-wording {
   text-align: center;
   padding: 200rpx 100rpx;
}

.page-body-info {
   display: flex;
   flex-direction: column;
   align-items: center;
   background-color: #fff;
   margin-bottom: 50rpx;
   width: 100%;
   padding: 50rpx 0 150rpx 0;
}

.page-body-title {
   margin-bottom: 100rpx;
   font-size: 32rpx;
}

.page-body-text {
   font-size: 30rpx;
   line-height: 26px;
   color: #ccc;
}

.page-body-text-small {
   font-size: 24rpx;
   color: #000;
   margin-bottom: 100rpx;
}

.page-body-form {
   width: 100%;
   background-color: #fff;
   display: flex;
   flex-direction: column;
   border: 1px solid #eee;
}

.page-body-form-item {
   display: flex;
   align-items: center;
   margin-left: 30rpx;
   border-bottom: 1px solid #eee;
   height: 88rpx;
   font-size: 34rpx;
}

.page-body-form-key {
   width: 180rpx;
   color: #000;
}

.page-body-form-value {
   flex-grow: 1;
}

.page-body-form-value .input-placeholder {
   color: #b2b2b2;
}

.page-body-form-picker {
   display: flex;
   justify-content: space-between;
   height: 100rpx;
   align-items: center;
   font-size: 36rpx;
   margin-left: 20rpx;
   padding-right: 20rpx;
   border-bottom: 1px solid #eee;
}

.page-body-form-picker-value {
   color: #ccc;
}

.page-body-buttons {
   width: 100%;
}

.page-body-button {
   margin: 25rpx;
}

.page-body-button image {
   width: 150rpx;
   height: 150rpx;
}

.page-footer {
   text-align: center;
   color: #1aad19;
   font-size: 24rpx;
   margin: 20rpx 0;
}

